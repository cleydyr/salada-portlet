<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="br.ufpe.cin.da.salada">
	<author>thecleydyr</author>
	<namespace>salada</namespace>
    <entity name="Sorteio" remote-service="false" local-service="true">
        <column name="sorteioId" type="long" primary="true"></column>
        <column name="dataAbertura" type="Date"></column>
        <column name="dataFechamento" type="Date"></column>
        <column name="nome" type="String"></column>
    </entity>
    <entity name="Pagamento" remote-service="false" trash-enabled="false"></entity>
    <entity name="Armario" remote-service="false"></entity>
    <entity name="Inscricao" remote-service="false"></entity>
    <entity name="Mensagem"></entity>
</service-builder>